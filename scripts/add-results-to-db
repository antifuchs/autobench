#!/bin/sh -e

. `dirname $0`/../functions

LOG=${1:-/dev/stdout}

cd $BASE

sbcl $COMMON_OPTS --load txt-sql.lisp --eval '(sb-ext:quit)' 2>>$LOG >&2

for i in  $BASE/to-import/*; do
	mkdir -p $BASE/to-keep/`basename $i`
	mv $i/* $BASE/to-keep/`basename $i` || true
done

# arch-tag: "10040901-ff31-11d8-8b1b-000c76244c24"
